{
    "dev": {
        "app_function": "simple_wsgi.application",
        "aws_region": "ap-southeast-2",
        "django_settings": "myproject.settings",
        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "profile_name": "default",
        "project_name": "myproject",
        "runtime": "python3.12",
        "s3_bucket": "myproject-static-files",
        "vpc_config": {
            "SubnetIds": ["subnet-0531f9eb4671e8c76", "subnet-09ba31f3a7ac08514", "subnet-0f3c066c42be0f4cf"]  // Replace with your actual subnet IDs
            "SecurityGroupIds": ["sg-0aaf41db4f0f5f3b3"]  // Replace with your actual security group ID
        },
        "environment_variables": {
            "DJANGO_SETTINGS_MODULE": "myproject.settings"
        },
        "include": [
            "mysqlclient"
        ]
    }
}

